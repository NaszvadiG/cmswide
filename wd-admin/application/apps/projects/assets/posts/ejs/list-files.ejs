<div class="row">
    <%
    if (data) {
        var files = data.files;
        var total_files = files.length;
        var total = data.total;
        var pagination = data.pagination;
        if(total_files){
            for(var i=0; i < total_files; i++){
                var file = files[i].file;
                %>
                <div class="col-sm-3 file" data-file="<%=file%>">
                    <div class="image-file">
                        <a href="#" class="btn-view-file" data-toggle="modal" data-target="#details">
                            <img src="<%=url%>apps/gallery/image/thumb/<%=file%>" title="<%=file%>" alt="<%=file%>" class="img-responsive">
                        </a>
                        <div class="options-file">
                            <a href="#" class="btn btn-primary btn-add-file"><span class="fa fa-plus"></span></a>
                        </div>
                    </div>
                    <div class="name-file"><%=file%></div>
                </div>
                <%
            }
            if (pagination) { 
                %>
                <div class="col-sm-12">
                    <ul class="pagination pagination-sm"><%=pagination%></ul>
                </div>
                <% 
            } 
        }else{
        %>
        <div class="col-sm-12">Nenhum arquivo foi encontrado.</div>    
        <%
    }
    } else {
    %>
    <div class="col-sm-12">Nenhum arquivo foi encontrado.</div>    
    <%
    }
    %>
</div>